      <node role="visibility" roleId="3vt2.1238599591623" type="3vt2.PublicVisibility" typeId="3vt2.1240402865704" id="3959497594242333590" />
      <node role="asDoc" roleId="3vt2.3043663067530528325" type="3vt2.AsDoc" typeId="3vt2.3043663067530528315" id="3959497594242455963">
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242455964">
          <property name="value" nameId="3vt2.3043663067530529475" value=" Specifies whether to use a compressed video stream for a local view of the camera. This method is applicable only if you are transmitting video using Flash Media Server; setting &lt;code&gt;compress&lt;/code&gt; to &lt;code&gt;true&lt;/code&gt; lets you see more precisely how the video will appear to users when they view it in real time." />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242455965">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;Although a compressed stream is useful for testing purposes, such as previewing video quality settings, it has a significant processing cost, because the local view is not simply compressed; it is compressed, edited for transmission as it would be over a live connection, and then decompressed for local viewing.&lt;/p&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242455966">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;To set the amount of compression used when you set &lt;code&gt;compress&lt;/code&gt; to &lt;code&gt;true&lt;/code&gt;, use &lt;code&gt;Camera.setQuality()&lt;/code&gt;.&lt;/p&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242455967">
          <property name="value" nameId="3vt2.3043663067530529475" value=" @param compress Specifies whether to use a compressed video stream (&lt;code&gt;true&lt;/code&gt;) or an uncompressed stream (&lt;code&gt;false&lt;/code&gt;) for a local view of what the camera is receiving." />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242455968">
          <property name="value" nameId="3vt2.3043663067530529475" value=" @see #setQuality()" />
        </node>
      </node>
