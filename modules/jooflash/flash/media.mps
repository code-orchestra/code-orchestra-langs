    <node role="asDoc" roleId="3vt2.3043663067530528325" type="3vt2.AsDoc" typeId="3vt2.3043663067530528315" id="3959497594242333706">
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333707">
        <property name="value" nameId="3vt2.3043663067530529475" value=" Use the Microphone class to monitor or capture audio from a microphone." />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333708">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;To get a reference to a Microphone instance, use the &lt;code&gt;Microphone.getMicrophone()&lt;/code&gt; method.&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333709">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;You can send microphone audio to a Flash Media Server. You can send the audio stream directly to the device audio output for local playback. You can also capture the audio stream for local recording or processing.&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333710">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;&lt;b&gt;Sending microphone audio to Flash Media Server&lt;/b&gt;&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333711">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;Use the NetConnection and NetStream classes to transmit the audio stream to Flash Media Server. Flash Media Server can send the audio to other servers and broadcast it to other clients running Flash Player or Adobe AIR.&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333712">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;&lt;b&gt;Playing microphone audio&lt;/b&gt;&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333713">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;Call the Microphone &lt;code&gt;setLoopback()&lt;/code&gt; method to route the microphone audio directly to the computer or device audio output. Uncontrolled audio feedback is an inherent danger and is likely to occur whenever the audio output can be picked up by the microphone input. The &lt;code&gt;setUseEchoSuppression()&lt;/code&gt; method can reduce, but not eliminate, the risk of feedback amplification.&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333714">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;&lt;b&gt;Capturing microphone audio&lt;/b&gt;&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333715">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;To capture microphone audio, listen for the &lt;code&gt;sampleData&lt;/code&gt; events dispatched by a Microphone instance. The SampleDataEvent object dispatched for this event contains the audio data.&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333716">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;For information about capturing video, see the Camera class.&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333717">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;&lt;b&gt;Runtime microphone support&lt;/b&gt;&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333718">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;The Microphone class is not supported in Flash Player running in a mobile browser.&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333719">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;&lt;i&gt;AIR profile support:&lt;/i&gt; The Microphone class is supported on desktop operating systems, but it is not supported on all mobile devices. It is not supported on AIR for TV devices. See &lt;a href=&quot;http://help.adobe.com/en_US/air/build/WS144092a96ffef7cc16ddeea2126bb46b82f-8000.html&quot;&gt;AIR Profile Support&lt;/a&gt; for more information regarding API support across multiple profiles.&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333720">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;You can test for support at run time using the &lt;code&gt;Microphone.isSupported&lt;/code&gt; property. Note that for AIR for TV devices, &lt;code&gt;Microphone.isSupported&lt;/code&gt; is &lt;code&gt;true&lt;/code&gt; but &lt;code&gt;Microphone.getMicrophone()&lt;/code&gt; always returns &lt;code&gt;null&lt;/code&gt;.&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333721">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;&lt;b&gt;Privacy controls&lt;/b&gt;&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333722">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;Flash Player displays a Privacy dialog box that lets the user choose whether to allow or deny access to the microphone. Your application window size must be at least 215 x 138 pixels, the minimum size required to display the dialog box, or access is denied automatically.&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333723">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;Content running in the AIR application sandbox does not need permission to access the microphone and no dialog is displayed. AIR content running outside the application sandbox does require permission and the Privacy dialog is displayed.&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333724">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;&lt;a href=&quot;http://help.adobe.com/en_US/FlashPlatform/reference/actionscript/3/flash/media/Microphone.html#includeExamplesSummary&quot;&gt;View the examples&lt;/a&gt;&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242333725">
