        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242456186">
          <property name="value" nameId="3vt2.3043663067530529475" value=" The codec to use for compressing audio. Available codecs are Nellymoser (the default) and Speex. The enumeration class &lt;code&gt;SoundCodec&lt;/code&gt; contains the various values that are valid for the &lt;code&gt;codec&lt;/code&gt; property." />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242456187">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;If you use the Nellymoser codec, you can set the sample rate using &lt;code&gt;Microphone.rate()&lt;/code&gt;. If you use the Speex codec, the sample rate is set to 16 kHz.&lt;/p&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242456188">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;Speex includes voice activity detection (VAD) and automatically reduces bandwidth when no voice is detected. When using the Speex codec, Adobe recommends that you set the silence level to 0. To set the silence level, use the &lt;code&gt;Microphone.setSilenceLevel()&lt;/code&gt; method.&lt;/p&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242456189">
          <property name="value" nameId="3vt2.3043663067530529475" value=" @see #setSilenceLevel()" />
        </node>
      </node>
