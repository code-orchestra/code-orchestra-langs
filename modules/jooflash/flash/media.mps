    <node role="asDoc" roleId="3vt2.3043663067530528325" type="3vt2.AsDoc" typeId="3vt2.3043663067530528315" id="3959497594242334249">
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242334250">
        <property name="value" nameId="3vt2.3043663067530529475" value=" The SoundLoaderContext class provides security checks for files that load sound. SoundLoaderContext objects are passed as an argument to the constructor and the &lt;code&gt;load()&lt;/code&gt; method of the Sound class." />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242334251">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;When you use this class, consider the following security model:&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242334252">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;ul&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242334253">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;li&gt;Loading and playing a sound is not allowed if the calling file is in a network sandbox and the sound file to be loaded is local.&lt;/li&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242334254">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;li&gt;By default, loading and playing a sound is not allowed if the calling is local and tries to load and play a remote sound. A user must grant explicit permission to allow this.&lt;/li&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242334255">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;li&gt;Certain operations dealing with sound are restricted. The data in a loaded sound cannot be accessed by a file in a different domain unless you implement a URL policy file. Sound-related APIs that fall under this restriction are the &lt;code&gt;Sound.id3&lt;/code&gt; property and the &lt;code&gt;SoundMixer.computeSpectrum()&lt;/code&gt;, &lt;code&gt;SoundMixer.bufferTime&lt;/code&gt;, and &lt;code&gt;SoundTransform()&lt;/code&gt; methods.&lt;/li&gt;&lt;/ul&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242334256">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;However, in Adobe AIR, content in the &lt;code&gt;application&lt;/code&gt; security sandbox (content installed with the AIR application) are not restricted by these security limitations.&lt;/p&gt;" />
      </node>
      <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242334257">
        <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;For more information related to security, see the Flash Player Developer Center Topic: &lt;a href=&quot;http://www.adobe.com/go/devnet_security_en&quot;&gt;Security&lt;/a&gt;.&lt;/p&gt;" />
      </node>
    </node>
    <node role="field" roleId="3vt2.1239651982107" type="3vt2.FieldDeclaration" typeId="3vt2.1239652032087" id="~SoundLoaderContext#bufferTime">
      <property name="isFinal" nameId="3vt2.525509775922074545" value="false" />
      <property name="name" nameId="tpck.1169194664001" value="bufferTime" />
      <node role="asDoc" roleId="3vt2.3043663067530528325" type="3vt2.AsDoc" typeId="3vt2.3043663067530528315" id="3959497594242334260">
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242334261">
          <property name="value" nameId="3vt2.3043663067530529475" value=" The number of milliseconds to preload a streaming sound into a buffer before the sound starts to stream." />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242334262">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;Note that you cannot override the value of &lt;code&gt;SoundLoaderContext.bufferTime&lt;/code&gt; by setting the global &lt;code&gt;SoundMixer.bufferTime&lt;/code&gt; property. The &lt;code&gt;SoundMixer.bufferTime&lt;/code&gt; property affects the buffer time for embedded streaming sounds in a SWF file and is independent of dynamically created Sound objects (that is, Sound objects created in ActionScript).&lt;/p&gt;" />
        </node>
      </node>
