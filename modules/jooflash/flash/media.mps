    <node role="method" roleId="3vt2.1238599729748" type="3vt2.InstanceGetterDeclaration" typeId="3vt2.1238599790300" id="3959497594242334732">
      <property name="name" nameId="tpck.1169194664001" value="deblocking" />
      <property name="isNative" nameId="3vt2.3838062322761349206" value="false" />
      <node role="returnType" roleId="3vt2.3618339097803723484" type="3vt2.ClassifierType" typeId="3vt2.1237649696613" id="3959497594242334737">
        <property name="name" nameId="tpck.1169194664001" value="int" />
        <link role="reference" roleId="3vt2.890882911603824802" targetNodeId="ufr.9027924984699862785" resolveInfo="int" />
      </node>
      <node role="visibility" roleId="3vt2.1238599591623" type="3vt2.PublicVisibility" typeId="3vt2.1240402865704" id="3959497594242334738" />
      <node role="asDoc" roleId="3vt2.3043663067530528325" type="3vt2.AsDoc" typeId="3vt2.3043663067530528315" id="3959497594242458544">
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242458545">
          <property name="value" nameId="3vt2.3043663067530529475" value=" Indicates the type of filter applied to decoded video as part of post-processing. The default value is 0, which lets the video compressor apply a deblocking filter as needed." />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242458546">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;Compression of video can result in undesired artifacts. You can use the &lt;code&gt;deblocking&lt;/code&gt; property to set filters that reduce blocking and, for video compressed using the On2 codec, ringing.&lt;/p&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242458547">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;&lt;i&gt;Blocking&lt;/i&gt; refers to visible imperfections between the boundaries of the blocks that compose each video frame. &lt;i&gt;Ringing&lt;/i&gt; refers to distorted edges around elements within a video image.&lt;/p&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242458548">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;Two deblocking filters are available: one in the Sorenson codec and one in the On2 VP6 codec. In addition, a deringing filter is available when you use the On2 VP6 codec. To set a filter, use one of the following values:&lt;/p&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242458549">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;ul&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242458550">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;li&gt;0—Lets the video compressor apply the deblocking filter as needed.&lt;/li&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242458551">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;li&gt;1—Does not use a deblocking filter.&lt;/li&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242458552">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;li&gt;2—Uses the Sorenson deblocking filter.&lt;/li&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242458553">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;li&gt;3—For On2 video only, uses the On2 deblocking filter but no deringing filter.&lt;/li&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242458554">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;li&gt;4—For On2 video only, uses the On2 deblocking and deringing filter.&lt;/li&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242458555">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;li&gt;5—For On2 video only, uses the On2 deblocking and a higher-performance On2 deringing filter.&lt;/li&gt;&lt;/ul&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242458556">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;If a value greater than 2 is selected for video when you are using the Sorenson codec, the Sorenson decoder defaults to 2.&lt;/p&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242458557">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;Using a deblocking filter has an effect on overall playback performance, and it is usually not necessary for high-bandwidth video. If a user's system is not powerful enough, the user may experience difficulties playing back video with a deblocking filter enabled.&lt;/p&gt;" />
        </node>
      </node>
