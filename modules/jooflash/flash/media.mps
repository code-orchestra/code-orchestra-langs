          <property name="value" nameId="3vt2.3043663067530529475" value=" An integer specifying the height of the video stream, in pixels." />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242458422">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;You may want to use this property, for example, to ensure that the user is seeing the video at the same height at which it was captured, regardless of the size of the &lt;code&gt;StageVideo.viewPort&lt;/code&gt; rectangle.&lt;/p&gt;" />
        </node>
      </node>
      <node role="body" roleId="3vt2.3618339097803723486" type="3vt2.StatementList" typeId="3vt2.1237648173643" id="3959497594242458423">
