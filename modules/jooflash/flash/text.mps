    <node role="staticField" roleId="3vt2.1239652787843" type="3vt2.StaticFieldDeclaration" typeId="3vt2.1239652745040" id="~FontType#EMBEDDED">
      <property name="isFinal" nameId="3vt2.525509775922074545" value="true" />
      <property name="name" nameId="tpck.1169194664001" value="EMBEDDED" />
      <node role="asDoc" roleId="3vt2.3043663067530528325" type="3vt2.AsDoc" typeId="3vt2.3043663067530528315" id="3959497594242344586">
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242344587">
          <property name="value" nameId="3vt2.3043663067530529475" value=" Indicates that this is an embedded font. Font outlines are embedded in the published SWF file." />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242344588">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;Text fields that use embedded fonts are always displayed in the chosen font, whether or not that font is installed on the playback system. Also, text fields that use embedded fonts are always anti-aliased (smoothed). You can select the amount of anti-aliasing you want by using the &lt;code&gt;TextField.antiAliasType property&lt;/code&gt;.&lt;/p&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242344589">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;One drawback to embedded fonts is that they increase the size of the SWF file.&lt;/p&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242344590">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;p&gt;Fonts of type &lt;code&gt;EMBEDDED&lt;/code&gt; can only be used by TextField. If flash.text.engine classes are directed to use such a font they will fall back to device fonts.&lt;/p&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242344591">
          <property name="value" nameId="3vt2.3043663067530529475" value=" @see TextField#embedFonts" />
        </node>
      </node>
