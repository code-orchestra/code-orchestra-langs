          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;td&gt;&amp;amp;amp;&lt;/td&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242479194">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;td&gt;&amp;amp; (ampersand)&lt;/td&gt;&lt;/tr&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242479195">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;tr&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242479196">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;td&gt;&amp;amp;quot;&lt;/td&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242479197">
          <property name="value" nameId="3vt2.3043663067530529475" value=" &lt;td&gt;&quot; (double quotes)&lt;/td&gt;&lt;/tr&gt;" />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242479198">
