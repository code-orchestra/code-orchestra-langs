        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242481548">
          <property name="value" nameId="3vt2.3043663067530529475" value=" @return &lt;code&gt;true&lt;/code&gt; if a compatible embedded font is available, otherwise &lt;code&gt;false&lt;/code&gt;." />
