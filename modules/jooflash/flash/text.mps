          <property name="value" nameId="3vt2.3043663067530529475" value=" Extends the CSS parsing capability. Advanced developers can override this method by extending the StyleSheet class." />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242478207">
          <property name="value" nameId="3vt2.3043663067530529475" value=" @param formatObject An object that describes the style, containing style rules as properties of the object, or &lt;code&gt;null&lt;/code&gt;." />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242478208">
          <property name="value" nameId="3vt2.3043663067530529475" value=" @return A TextFormat object containing the result of the mapping of CSS rules to text format properties." />
