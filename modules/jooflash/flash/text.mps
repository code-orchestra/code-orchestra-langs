      <node role="asDoc" roleId="3vt2.3043663067530528325" type="3vt2.AsDoc" typeId="3vt2.3043663067530528315" id="3959497594242480757">
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242480758">
          <property name="value" nameId="3vt2.3043663067530529475" value=" Given a character index, returns the index of the first character in the same paragraph." />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242480759">
          <property name="value" nameId="3vt2.3043663067530529475" value=" @param charIndex The zero-based index value of the character (for example, the first character is 0, the second character is 1, and so on)." />
        </node>
        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242480760">
