        <node role="line" roleId="3vt2.3043663067530529483" type="3vt2.AsDocLine" typeId="3vt2.3043663067530529472" id="3959497594242483661">
          <property name="value" nameId="3vt2.3043663067530529475" value=" @param caseSensitive Specifies whether the text must match the case of the string in &lt;code&gt;textToFind&lt;/code&gt;." />
